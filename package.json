    "nuxt": "^2.15.6",
    "webpack": "4.44.2",
    "webpack-node-externals": "2.5.2"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "bootstrap": "^5.1.1",
    "sass": "^1.42.1",
